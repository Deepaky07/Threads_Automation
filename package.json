{
  "name": "threads-automation-bot",
  "version": "2.0.0",
  "description": "Threads automation bot with persistent session management",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "session:list": "node scripts/sessionManager.cli.js list",
    "session:stats": "node scripts/sessionManager.cli.js stats",
    "session:cleanup": "node scripts/sessionManager.cli.js cleanup",
    "session:check": "node scripts/sessionManager.cli.js check",
    "session:delete": "node scripts/sessionManager.cli.js delete",
    "session:info": "node scripts/sessionManager.cli.js info",
    "bot:search": "node bots/searchBot.js",
    "bot:notification": "node bots/notificationBot.js",
    "bot:flow": "node bots/flowBot.js",
    "db:connect": "mongo threads_bot",
    "db:backup": "mongodump --db threads_bot --out ./backups/$(date +%Y%m%d_%H%M%S)",
    "db:restore": "mongorestore --db threads_bot",
    "logs:view": "tail -f logs/threads_bot.log",
    "logs:clear": "rm -rf logs/*.log"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^164.1.0",
    "helmet": "^7.1.0",
    "js-cookie": "^3.0.5",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "puppeteer": "^24.27.0",
    "recharts": "^3.3.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "threads",
    "automation",
    "bot",
    "puppeteer",
    "session-management"
  ],
  "author": "Your Name",
  "license": "MIT"
}
